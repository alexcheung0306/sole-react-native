{
  "name": "sole-native",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "dev": "expo start --dev-client",
    "clear": "node scripts/update-ip.js && expo start --clear",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "expo start --web",
    "start:dev": "APP_ENV=development expo start --clear",
    "start:staging": "APP_ENV=staging expo start --clear",
    "start:prod": "APP_ENV=production expo start --clear",
    "build:dev": "eas build --profile development --platform all",
    "build:dev:ios": "eas build --profile development --platform ios",
    "build:dev:android": "eas build --profile development --platform android",
    "build:staging": "eas build --profile staging --platform all",
    "build:staging:ios": "eas build --profile staging --platform ios",
    "build:staging:android": "eas build --profile staging --platform android",
    "build:prod": "eas build --profile production --platform all",
    "build:prod:ios": "eas build --profile production --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "submit:prod:ios": "eas submit --platform ios --profile production",
    "submit:prod:android": "eas submit --platform android --profile production"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.14.28",
    "@expo/html-elements": "^0.12.5",
    "@expo/vector-icons": "^15.0.3",
    "@gluestack-ui/core": "^3.0.0",
    "@gluestack-ui/utils": "^3.0.0",
    "@gluestack/ui-next-adapter": "^3.0.2",
    "@gorhom/bottom-sheet": "^5.2.6",
    "@legendapp/motion": "^2.4.0",
    "@react-navigation/native": "^7.0.3",
    "@t3-oss/env-core": "^0.13.8",
    "@tanstack/react-query": "^5.87.1",
    "@tanstack/react-query-devtools": "^5.87.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "date-fns": "^3.6.0",
    "dotenv": "^17.2.2",
    "expo": "~54.0.25",
    "expo-auth-session": "~7.0.9",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.9",
    "expo-constants": "~18.0.10",
    "expo-dev-client": "^6.0.18",
    "expo-font": "~14.0.9",
    "expo-image": "~3.0.10",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.9",
    "expo-location": "~19.0.7",
    "expo-media-library": "~18.2.0",
    "expo-router": "~6.0.15",
    "expo-secure-store": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "formik": "^2.4.8",
    "lucide-react-native": "^0.542.0",
    "nativewind": "^4.1.23",
    "react": "19.1.0",
    "react-aria": "^3.33.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-calendar-picker": "^6.1.5",
    "react-native-date-picker": "^5.0.13",
    "react-native-deck-swiper": "^2.0.19",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-image-crop-picker": "^0.51.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-reanimated-carousel": "^4.0.3",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-view-shot": "4.0.3",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "react-stately": "^3.39.0",
    "tailwind-variants": "^0.1.20",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "ajv": "^8.12.0",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "@gluestack-ui/core",
          "@gluestack-ui/utils",
          "@gluestack/ui-next-adapter",
          "@t3-oss/env-core",
          "@tanstack/react-query-devtools",
          "dotenv",
          "react-aria",
          "react-stately",
          "tailwind-variants"
        ],
        "listUnknownPackages": false
      }
    }
  },
  "private": true
}